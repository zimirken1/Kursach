generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String   @id @default(cuid())
  email       String   @unique
  password    String
  token       Token?   
}

model Token {
  id            String  @id @default(cuid()) 
  refreshToken  String
  userId        String  @unique 
  user          User    @relation(fields: [userId], references: [id])
}